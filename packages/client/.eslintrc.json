{
	"extends": [
		"@eslint-sets/vue3",
		"@eslint-sets/vue3-ts",
		"./.eslintrc-auto-import.json"
	],
	"plugins": [
		"file-progress",
		"prettier",
		"unused-imports",
		"eslint-plugin-unused-imports"
	],
	"rules": {
		"file-progress/activate": 1,
		"no-unused-vars": "off",
		"unused-imports/no-unused-imports": "error"
	}
}
